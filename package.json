{
  "name": "consultation-static",
  "version": "2.0.0",
  "description": "",
  "main": "default",
  "scripts": {
    "clean": "rm -rf dist",
    "eleventy:dev": "cross-env ELEVENTY_ENV=dev npx eleventy --watch",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production npx eleventy",
    "scss:build": "sass --style=expanded --embed-sources --quiet --load-path=node_modules src/scss/:dist/assets/css && postcss dist/assets/css/*.css --replace",
    "scss:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run scss:build'",
    "images:dev": "cpx \"src/images/**/*\" dist/assets/images --watch --verbose",
    "images:prod": "cpx \"src/images/**/*\" dist/assets/images",
    "fonts:dev": "cpx \"src/fonts/**/*\" dist/assets/fonts --watch --verbose",
    "fonts:prod": "cpx \"src/fonts/**/*\" dist/assets/fonts",
    "js:dev": "webpack --watch --mode=development",
    "js:prod": "webpack --mode=production",
    "serve": "wait-on dist/index.html && browser-sync start --server \"dist\" --files \"dist\" --no-notify",
    "start": "npm run clean && concurrently \"npm:scss:*\" \"npm:js:dev\" \"npm:images:dev\" \"npm:fonts:dev\" \"npm:eleventy:dev\" \"npm:serve\"",
    "build": "npm run clean && npm run scss:build && npm run js:prod && npm run images:prod && npm run fonts:prod && npm run eleventy:prod",
    "test:build": "node test-before-build.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesforddesign/consultation-static.git"
  },
  "author": "JFD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesforddesign/consultation-static/issues"
  },
  "homepage": "https://github.com/jamesforddesign/consultation-static#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@babel/preset-env": "^7.24.7",
    "@bugsnag/js": "^7.23.0",
    "@fancyapps/ui": "^5.0.36",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.3",
    "browser-sync": "^3.0.3",
    "child_process": "^1.0.2",
    "chokidar-cli": "^3.0.0",
    "click-outside": "^2.0.2",
    "concurrently": "^8.2.2",
    "cpx2": "^7.0.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "flickity": "^2.2.2",
    "flickity-as-nav-for": "^2.0.1",
    "flickity-fade": "1.0.0",
    "flickity-imagesloaded": "^2.0.0",
    "flickity-sync": "^2.0.0",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "https": "^1.0.0",
    "js-yaml": "^4.1.0",
    "load-google-maps-api": "^2.0.2",
    "marked": "^13.0.3",
    "medium-zoom": "^1.1.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "pretty": "^2.0.0",
    "slugify": "^1.6.6",
    "typemate": "^0.7.1",
    "wait-on": "^7.2.0",
    "whatwg-fetch": "^3.6.19"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "mocha": "^10.7.0",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
